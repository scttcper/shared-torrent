    "@types/node": "22.14.0",
    "typescript": "5.8.2",
    "undici": "^7.7.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.7.1"
